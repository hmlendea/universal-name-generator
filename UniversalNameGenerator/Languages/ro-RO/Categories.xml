<?xml version="1.0" encoding="utf-8"?>
<ArrayOfCategory xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Category>
    <Id>settlement</Id>
    <Name>Settlement names</Name>
    <Wordlists>
      <string>bases</string>
      <string>filters</string>
      <string>suffixes</string>
    </Wordlists>
    <GenerationSchema>{bases}{suffixes}</GenerationSchema>
  </Category>
  <Category>
    <Id>person-male</Id>
    <Name>Male names</Name>
    <Wordlists>
      <string>firstnames</string>
      <string>surnames</string>
    </Wordlists>
    <GenerationSchema>{firstnames} {surnames}</GenerationSchema>
  </Category>
</ArrayOfCategory>
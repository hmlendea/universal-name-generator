<?xml version="1.0" encoding="utf-8"?>
<ArrayOfGenerationSchema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <GenerationSchema>
    <Id>asian-settlements</Id>
    <Name>Asian settlement names</Name>
    <Schema>{randomiser,,asian-settlements-bases|asian-settlements-suffixes}</Schema>
  </GenerationSchema>
  <GenerationSchema>
    <Id>ro-birds</Id>
    <Name>Romanian bird species names</Name>
    <Schema>{markov,ro-birds}</Schema>
  </GenerationSchema>
  <GenerationSchema>
    <Id>ro-colours</Id>
    <Name>Romanian colours names</Name>
    <Schema>{markov,ro-colours}</Schema>
  </GenerationSchema>
  <GenerationSchema>
    <Id>ro-fauna</Id>
    <Name>Romanian fauna species names</Name>

<?xml version="1.0" encoding="utf-8"?>
<ArrayOfCategory xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Category>
    <Id>settlements</Id>
    <Name>Settlement names</Name>
    <Filterlist>settlements_filters</Filterlist>
    <Wordlists>
      <string>settlements_bases</string>
      <string>settlements_filters</string>
      <string>settlements_suffixes</string>
    </Wordlists>
    <GenerationSchema>{randomiser,settlements_bases|settlements_suffixes}</GenerationSchema>
  </Category>
  <Category>
    <Id>persons-male</Id>
    <Name>Male person names</Name>
    <Filterlist>persons_filters</Filterlist>

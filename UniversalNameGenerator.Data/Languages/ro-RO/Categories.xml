      <string>persons_males</string>
      <string>persons_surnames</string>
    </Wordlists>
    <GenerationSchema>{persons_males} {persons_surnames}</GenerationSchema>
  </Category>
  <Category>
    <Id>persons-female</Id>
    <Name>Female persons names</Name>
    <Filterlist>persons_filters</Filterlist>
    <Wordlists>
      <string>persons_females</string>
      <string>persons_surnames</string>
    </Wordlists>
    <GenerationSchema>{persons_females} {persons_surnames}</GenerationSchema>
  </Category>
</ArrayOfCategory>